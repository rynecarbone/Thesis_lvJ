	  errorLo = var->getErrorLo() / (w->var("nominalLumi")->getVal() * LumiRelError); 
	}
	
	TString vname=var->GetName();
	vname.ReplaceAll("alpha_","");
	vname.ReplaceAll("gamma_","");
	vname.ReplaceAll("Lumi","Luminosity");
